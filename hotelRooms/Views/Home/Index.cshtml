﻿@using HotelH2.Controllers;
@{
    ViewData["Title"] = "Home Page";
}
@model List<Rooms>

<div class="text-center">
    <h1 class="display-4">Welcome to HotelH2</h1>

    <table>
        <tr>
            <th>Room ID</th>
            <th>RoomType</th>
            <th>Price</th>
            <th>Vacancy</th>
            <th>Room for</th>

        </tr>
        @foreach (var room in Model)
        {
            <tr>
                <td>@room.ID</td>
                <td>@room.Type</td>
                <td>@room.Price</td>
                <td>@room.occipied</td>
                <td>@room.maxPersoncount</td>
                <td>@room.startdate</td>
                <td>@room.slutdate</td>
                <td>@room.temp</td>
            </tr>
        }
    </table>
</div>

